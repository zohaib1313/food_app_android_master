<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bigbird.foodorderingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodOrderingApp">
        <activity android:name=".activities.kitchen_owner.AddNewDishDialog"></activity>
        <activity android:name=".activities.user.OrderPlacedWaitActivity" />
        <activity android:name=".activities.user.UserDashboardActivity" />
        <activity android:name=".activities.kitchen_owner.KitchenOwnerDashboardActivity" />
        <activity android:name=".activities.admin.AdminDashboardActivity" />
        <activity android:name=".activities.admin.SignUpAdminActivity" />
        <activity android:name=".activities.user.SignUpUserActivity" />
        <activity android:name=".activities.TermsAndConditionActivity" />
        <activity android:name=".activities.ChooseUserLoginTypeActivity" />
        <activity android:name=".activities.kitchen_owner.SignUpKitchenOwnerActivity" />
        <activity android:name=".activities.SignInActivity" />
        <activity
            android:name=".activities.SplashActivity"
            android:theme="@style/full_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>